[flake8]
ignore = E266, E501, W503
max-line-length = 79
max-complexity = 12
select = B,C,E,F,W,T4,B9
exclude = .git, *migrations*, __pycache__
per-file-ignores =
    {{ project_name }}/conf/settings/local.py:F405
    {{ project_name }}/conf/settings/production.py:F405
    {{ project_name }}/conf/settings/test.py:F405